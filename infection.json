{
	"source": {
		"directories": [
			"./Mezon/*"
		],
		"excludes": [
			"Tests"
		]
	},
	"phpUnit": {
		"customPath": ".\/vendor\/phpunit\/phpunit\/phpunit"
	},
	"logs": {
		"text": ".\/tmp\/infection-text.txt",
		"summary": "\/tmp\/summary-log.txt",
		"debug": "\/tmp\/debug-log.txt",
		"perMutator": "\/tmp\/per-mutator-log.txt"
